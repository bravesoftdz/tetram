<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="8.0" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
          minHeight="-Infinity" minWidth="-Infinity" stylesheets="@buttonsbar.css" xmlns="http://javafx.com/javafx/8"
          fx:controller="org.tetram.bdtheque.gui.controllers.includes.ButtonsBarController">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="-Infinity"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button fx:id="btOk" mnemonicParsing="false" text="Ok" GridPane.columnIndex="1"/>
        <Button fx:id="btCancel" mnemonicParsing="false" text="Annuler" GridPane.columnIndex="2"/>
        <Label fx:id="lbMessage" id="lbMessage" text="Label"/>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
</GridPane>
