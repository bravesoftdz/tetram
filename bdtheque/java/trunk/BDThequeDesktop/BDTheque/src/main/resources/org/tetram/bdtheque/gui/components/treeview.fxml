<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<TreeTableView xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="200.0"
               xmlns="http://javafx.com/javafx/8"
               fx:controller="org.tetram.bdtheque.gui.controllers.includes.TreeViewController" fx:id="treeTableView"
               showRoot="false" stylesheets="@treeview.css">
    <columnResizePolicy>
        <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
    <columns>
        <TreeTableColumn fx:id="column0" editable="false" sortable="false"/>
        <TreeTableColumn fx:id="column1" editable="false" maxWidth="68.0" minWidth="68.0" prefWidth="68.0"
                         resizable="false" sortable="false"/>
    </columns>
</TreeTableView>
