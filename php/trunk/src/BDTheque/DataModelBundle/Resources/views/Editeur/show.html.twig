<h1>Editeur</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Version</th>
            <td>{{ entity.version|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Nomediteur</th>
            <td>{{ entity.nomEditeur }}</td>
        </tr>
        <tr>
            <th>Siteweb</th>
            <td>{{ entity.siteWeb }}</td>
        </tr>
        <tr>
            <th>Initiale</th>
            <td>{{ entity.initiale }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('editeur') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('editeur_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('editeur_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
